{
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "dev"
  ],
  "reviewers": [
    "xknat"
  ],
  "ignoreDeps": [
    "com.viaversion:viaversion"
  ],
  "labels": [
    "renovate"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "org.spigotmc:spigot-api"
      ],
      "enabled": false
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ]
}
